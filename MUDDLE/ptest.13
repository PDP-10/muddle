<SETG FRAMEN 
  <FUNCTION (N)
   <COND (<0? .N> <FRAME>)
         (T <FRAME <FRAMEN <- .N 1>>>)>>>


<SETG PATH 
 <FUNCTION TL (START FINISH)
   <PROG ((VAL <FAILPOINT () <PATH1 .START .FINISH ()>
                    ()
                    <EXIT .TL <>>   >))
    <FINALIZE .TL>
    <RETURN .VAL>>>>


<SETG PATH1
 <FUNCTION  (START FINISH AVOID)
   <COND (<==? .START .FINISH>
          (.FINISH))
         (<MEMBER .START .AVOID>
          <FAIL>)
         (T (.START 
             !<PATH1
               <FAILPOINT FP (N (NODES <GET .START CONNECTED>))
                        <FAIL> ()
                        <COND (<EMPTY? .NODES> <FAIL>)
                              (<SET N <1 .NODES>>
                               <SET NODES <REST .NODES>>
                               <RESTORE .FP .N>)>   >
               .FINISH
               (.START !.AVOID)>))>>   >


